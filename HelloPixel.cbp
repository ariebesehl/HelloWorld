			<Add option="-s" />
			<Add option="-flto" />
		</Linker>
		<Unit filename="HelloPixel.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="HelloPixel.h" />
		<Unit filename="HelloPixel.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
